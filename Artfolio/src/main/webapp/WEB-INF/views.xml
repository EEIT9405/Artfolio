<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="bountyIndext"
		class="org.springframework.web.servlet.view.RedirectView">
		<property name="url" value="/bounty/bountyIndex.jsp"></property>
		<property name="contextRelative" value="true"></property>
	</bean>
	
	<bean id="bountyPage"
		class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/bounty/BountyPage.jsp"></property>
	</bean>
	
	<bean id="picture"
		class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/picture.jsp"></property>
	</bean>
	
	<bean id="UploadIndex"
		class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/Upload/UploadIndex.jsp"></property>
	</bean>
	
	<bean id="point" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/point.jsp"/>
	</bean> 
	<bean id="traderesult" class="org.springframework.web.servlet.view.RedirectView">
		<property name="contextRelative" value="true" />
		<property name="url" value="/traderesult.jsp"/>
	</bean> 
	<bean id="donate" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/donate.jsp"/>
	</bean> 
	<bean id="work" class="org.springframework.web.servlet.view.RedirectView">
		<property name="contextRelative" value="true" />
		<property name="url" value="/work.jsp"/>
	</bean> 
	
</beans>
