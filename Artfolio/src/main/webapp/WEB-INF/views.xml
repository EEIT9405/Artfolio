<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	
	<bean id="picture"
		class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/picture.jsp"></property>
	</bean>
	
	<bean id="UploadIndex"
		class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/Upload/UploadIndex.jsp"></property>
	</bean>
	
	<bean id="point" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/WEB-INF/web/point.jsp"/>
	</bean> 
	<bean id="traderesult" class="org.springframework.web.servlet.view.RedirectView">
		<property name="contextRelative" value="true" />
		<property name="url" value="/traderesult.jsp"/>
	</bean> 
	<bean id="donate" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/WEB-INF/web/donate.jsp"/>
	</bean> 
	<bean id="upload" class="org.springframework.web.servlet.view.RedirectView">
		<property name="contextRelative" value="true" />
		<property name="url" value="/uploadresult.jsp"/>
	</bean> 
	
	<bean id="login" class="org.springframework.web.servlet.view.RedirectView">
		<property name="contextRelative" value="true" />
		<property name="url" value="/secure/login.jsp"/>
	</bean> 
	
	<bean id="targetPage" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/authorWorks.jsp"/>
	</bean>
	
	<bean id="index" class="org.springframework.web.servlet.view.RedirectView">
		<property name="contextRelative" value="true" />
		<property name="url" value="/index.jsp"/>
	</bean>
	
	<bean id="myWorks" class="org.springframework.web.servlet.view.RedirectView">
		<property name="contextRelative" value="true" />
		<property name="url" value="/myWorks.jsp"/>
	</bean> 
	
	<bean id="bountyPage"
		class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/bounty/BountyPage.jsp"></property>
	</bean>
	
	<bean id="bountyChange"
		class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/bounty/bountyChange.jsp"></property>
	</bean>
	
</beans>
